Model:       LG, Atlant, Haier
Freezer:     No Frost, Manual
Fridge:      No Frost, Manual
Compressor:  Standard, Inverter, Linear-inverter

if [Model] = "LG" then [Freezer] = "No Frost";
if [Model] = "Atlant" then [Compressor] = "Standard";
if [Model] = "LG"  and [Compressor] = "Standard" then [Fridge] = "No Frost";
if [Model] = "LG" and [Compressor] = "Linear-Inverter" then [Fridge] <> "Manual";
if [Model] = "Haier" then [Compressor] <> "Linear-inverter" and [Freezer] <> "Manual"; 
